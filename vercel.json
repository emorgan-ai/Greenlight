{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "cd greenlight && npm install && npm run build",
  "devCommand": "cd greenlight && npm run dev",
  "installCommand": "cd greenlight && npm install",
  "outputDirectory": "greenlight/.next",
  "framework": "nextjs",
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "OPENAI_API_KEY": "@openai_api_key",
    "NEXT_PUBLIC_API_URL": "@next_public_api_url"
  }
}
